<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ModifyPartController">
    <children>
        <Label layoutX="63.0" layoutY="89.0" text="Modify Part">
            <font>
                <Font name="System Bold" size="16.0" />
            </font>
        </Label>
        <RadioButton fx:id="houseModifyPart" layoutX="250.0" layoutY="93.0" mnemonicParsing="false" onAction="#houseOnClickedModifyPart" text="In-House" >
            <toggleGroup>
                <ToggleGroup fx:id="RadioButton" />
            </toggleGroup>
        </RadioButton>
        <RadioButton fx:id="outModifyPart" layoutX="451.0" layoutY="93.0" mnemonicParsing="false" onAction="#outOnClickedModifyPart" toggleGroup="$RadioButton" text="OutSourced" />
        <Label layoutX="96.0" layoutY="206.0" text="ID" />
        <Label layoutX="96.0" layoutY="253.0" text="Name" />
        <Label layoutX="96.0" layoutY="297.0" text="Inventory" />
        <Label layoutX="96.0" layoutY="350.0" text="Price" />
        <Label layoutX="96.0" layoutY="403.0" text="Max" />
        <Label layoutX="440.0" layoutY="403.0" text="Min" />
        <Label fx:id="machineIdModifyPart" layoutX="96.0" layoutY="453" text="Machine ID" />
        <TextField fx:id="idModifyPart" editable="false" layoutX="250.0" layoutY="202.0" />
        <TextField fx:id="nameModifyPart" layoutX="250.0" layoutY="249.0" />
        <TextField fx:id="inventoryModifyPart" layoutX="250.0" layoutY="293.0" />
        <TextField fx:id="priceModifyPart" layoutX="250.0" layoutY="346.0" />
        <TextField fx:id="maxModifyPart" layoutX="250.0" layoutY="399.0" />
        <TextField fx:id="minModifyPart" layoutX="500.0" layoutY="399.0" />
        <TextField fx:id="machIneModifyPart" layoutX="250.0" layoutY="449.0" />
        <Button fx:id="saveModifyPart" layoutX="414.0" layoutY="516.0" mnemonicParsing="false" onAction="#saveOnClickedModifyPart" text="Save" />
        <Button fx:id="cancelModifyPartPage" layoutX="500.0" layoutY="516.0" mnemonicParsing="false" onAction="#onClickCancelModifyPartPageButton" text="Cancel" />
    </children>
</AnchorPane>
